{
    "name": "symptothermapp-frontend",
    "private": true,
    "version": "2.8.0",
    "type": "module",
    "scripts": {
        "dev": "npm run gen-api && tsc -b && vite",
        "build": "tsc -b && vite build",
        "deploy": "npm run build && cd ../ && docker build -t 12ukas/symptothermapp:latest .  --push",
        "lint": "eslint .",
        "preview": "vite preview",
        "devserver": "cd ../src && gradlew bootRun --args='--spring.profiles.active=dev'",
        "gen-api": "cd ../src && gradlew generateOpenApiDocs && cd ../src-ui && node ./scripts/copyOpenApiSchema.js && npx @rtk-query/codegen-openapi openapi-config.ts",
        "release": "tsc -b && release-it",
        "version": "auto-changelog -p && git add CHANGELOG.md"
    },
    "dependencies": {
        "@mantine/core": "^7.13.4",
        "@mantine/dates": "^7.13.4",
        "@mantine/hooks": "^7.13.4",
        "@mantine/modals": "^7.13.4",
        "@mantine/notifications": "^7.13.4",
        "@reduxjs/toolkit": "^2.3.0",
        "dayjs": "^1.11.12",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.53.1",
        "react-markdown": "^9.0.1",
        "react-redux": "^9.1.2",
        "recharts": "^2.13.0",
        "symptothermapp-frontend": "file:"
    },
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@rtk-query/codegen-openapi": "^1.2.0",
        "@types/node": "^22.8.2",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react-swc": "^3.7.1",
        "auto-changelog": "^2.5.0",
        "esbuild-runner": "^2.2.2",
        "eslint": "^9.13.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.14",
        "globals": "^15.11.0",
        "postcss": "^8.4.41",
        "postcss-preset-mantine": "^1.17.0",
        "postcss-simple-vars": "^7.0.1",
        "release-it": "^17.10.0",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.12.1",
        "vite": "^5.4.10"
    }
}
