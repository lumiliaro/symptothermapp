{
    "name": "symptothermapp-frontend",
    "private": true,
    "version": "2.0.6",
    "type": "module",
    "scripts": {
        "dev": "npm run gen-api && tsc -b && vite",
        "build": "tsc -b && vite build",
        "deploy": "npm run build && cd ../ && docker build -t 12ukas/symptothermapp:latest .  --push",
        "lint": "eslint .",
        "preview": "vite preview",
        "devserver": "cd ../src && gradlew bootRun --args='--spring.profiles.active=dev'",
        "gen-api": "cd ../src && gradlew generateOpenApiDocs && cd ../src-ui && node copyOpenApiSchema.js && npx @rtk-query/codegen-openapi openapi-config.ts",
        "release": "tsc -b && release-it"
    },
    "dependencies": {
        "@mantine/core": "^7.12.1",
        "@mantine/dates": "^7.12.1",
        "@mantine/hooks": "^7.12.1",
        "@mantine/modals": "^7.12.1",
        "@mantine/notifications": "^7.12.1",
        "@reduxjs/toolkit": "^2.2.7",
        "dayjs": "^1.11.12",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.52.2",
        "react-redux": "^9.1.2",
        "recharts": "^2.12.7",
        "symptothermapp-frontend": "file:"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@rtk-query/codegen-openapi": "^1.2.0",
        "@types/node": "^22.5.5",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "esbuild-runner": "^2.2.2",
        "eslint": "^9.11.0",
        "eslint-plugin-react": "^7.35.0",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.9",
        "globals": "^15.9.0",
        "postcss": "^8.4.41",
        "postcss-preset-mantine": "^1.17.0",
        "postcss-simple-vars": "^7.0.1",
        "release-it": "^17.6.0",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.0.1",
        "vite": "^5.4.7"
    }
}
